{
  "manifest_version": 3,
  "name": "Smart Privacy",
  "version": "1.0",
  "description": "A smart privacy extension.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:9002/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' http://localhost:9002/;"
  }
}
